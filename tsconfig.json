{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/apis/*": ["apis/*"],
      "@/components/*": ["components/*"],
      "@/constants/*": ["constants/*"],
      "@/hooks/*": ["hooks/*"],
      "@/navigation/*": ["navigation/*"],
      "@/screens/*": ["screens/*"],
      "@/state/*": ["state/*"]
    }
  },
  "exclude": [
    "node_modules",
  ]
}
